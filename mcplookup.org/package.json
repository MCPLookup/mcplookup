{
  "name": "mcplookup-registry",
  "version": "1.0.0",
  "description": "Professional MCP Discovery Server - Enterprise-grade MCP Discovery Service with Flexible Storage Architecture",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "next build && tsc --project tsconfig.build.json",
    "start": "next start",
    "dev": "next dev",
    "dev:mcp": "tsx watch src/server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:storage": "tsx scripts/test-storage.ts",
    "test:redis": "tsx scripts/test-redis-storage.ts",
    "test:ai": "tsx scripts/test-ai-setup.js",
    "test:docker": "scripts/test-docker-redis.sh",
    "dev:setup": "scripts/dev-setup.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "redis:start": "scripts/redis-manager.sh start",
    "redis:stop": "scripts/redis-manager.sh stop",
    "redis:status": "scripts/redis-manager.sh status",
    "redis:cli": "scripts/redis-manager.sh cli",
    "redis:ui": "scripts/redis-manager.sh ui",
    "redis:flush": "scripts/redis-manager.sh flush",
    "redis:backup": "scripts/redis-manager.sh backup",
    "redis:sync": "tsx scripts/redis-sync.js",
    "redis:sync:dry": "tsx scripts/redis-sync.js --dry-run",
    "redis:sync:watch": "tsx scripts/redis-sync.js --watch",
    "redis:sync:reverse": "tsx scripts/redis-sync.js --reverse",
    "redis:sync:help": "tsx scripts/redis-sync.js --help",
    "setup:ai": "echo 'Choose your AI provider (OpenAI removed - too expensive!):' && echo 'TOGETHER_API_KEY=your-key-here (cheapest)' && echo 'OPENROUTER_API_KEY=your-key-here (most flexible, has free models)'",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --project tsconfig.typecheck.json",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "discovery",
    "registry",
    "ai-tools",
    "serverless",
    "storage-abstraction",
    "redis",
    "upstash",
    "vercel",
    "dns-verification",
    "real-time-discovery",
    "typescript",
    "pagination",
    "error-handling"
  ],
  "author": "MCPLookup.org",
  "license": "MIT",
